{
	"name": "forpipeline",
	"properties": {
		"activities": [
			{
				"name": "Get Metadata1",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "para_dataset",
						"type": "DatasetReference",
						"parameters": {
							"ds_filesystem": {
								"value": "@pipeline().parameters.ppf_filesystem",
								"type": "Expression"
							},
							"ds_directory": {
								"value": "@pipeline().parameters.ppf_directory_in",
								"type": "Expression"
							},
							"ds_filename": {
								"value": "@pipeline().parameters.ppf_filename",
								"type": "Expression"
							}
						}
					},
					"fieldList": [
						"childItems",
						"itemName",
						"itemType",
						"lastModified"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			}
		],
		"parameters": {
			"ppf_filesystem": {
				"type": "string"
			},
			"ppf_directory_in": {
				"type": "string"
			},
			"ppf_filename": {
				"type": "string"
			}
		},
		"annotations": []
	}
}